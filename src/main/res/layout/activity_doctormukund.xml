<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ba"
    tools:context=".doctormukund">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="203dp"
        android:layout_height="109dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="130dp"
        android:text="Dr.Mukund Deshpande"
        android:textColor="@color/colorWhite"
        android:textSize="35sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="86dp"
        android:layout_height="118dp"
        app:srcCompat="@drawable/mukund"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="40dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="192dp"
        android:layout_height="96dp"
        android:layout_below="@id/textView3"
        android:layout_marginLeft="140dp"
        android:text="MBBS,Gynaecologist       (Experience: 25+ years)        Works at Alexes Hospital ,          Manewadad Road,Nagpur"
        android:textColor="@color/white"
        android:textSize="15dp"
        tools:ignore="MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="129dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="181dp"
        android:layout_height="67dp"
        android:text="Alexes Multispecality and HealthCare."
        android:textColor="@color/white"
        android:layout_below="@id/textView6"
        android:layout_marginLeft="140dp"
        android:textSize="16dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="278dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="158dp"
        android:layout_height="41dp"
        android:text="7796301415."
        android:textColor="@color/white"
        android:layout_below="@id/textView7"
        android:layout_marginLeft="140dp"
        android:textSize="18dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="345dp" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text="Phone No : "
        android:layout_below="@id/editTextTextPersonName"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="336dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="103dp"
        android:layout_height="48dp"
        android:ems="10"
        android:layout_below="@id/imageView4"
        android:inputType="textPersonName"
        android:text="Hospital: "
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="271dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="296dp"
        android:layout_height="23dp"
        android:layout_below="@id/phone"
        android:layout_marginLeft="25dp"
        android:text="Available Between 9:00 am to 10:00 pm"
        android:textColor="@color/yellowLight"
        android:textSize="16dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="404dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="260dp"
        android:layout_height="49dp"
        android:layout_below="@id/textView9"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_bg"
        android:text="Book the Appointment"
        android:layout_marginLeft="50dp"
        android:textColor="@color/colorWhite"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="334dp"
        android:layout_height="35dp"
        android:layout_below="@id/button"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:text="Schedule the appointment according to your convinience and Save the waitlist time!"
        android:textAlignment="center"
        android:textColor="@color/teal_200"
        android:textSize="12dp"
        tools:ignore="MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="516dp" />

</RelativeLayout>